        android:layout_marginTop="28dp"
        android:background="@android:color/holo_red_dark"
        android:text="赤6番"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/red_2" />

    <Button
        android:id="@+id/blue_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:background="@android:color/holo_blue_dark"
        android:text="青5番"
        app:layout_constraintStart_toEndOf="@+id/red_2"
        app:layout_constraintTop_toTopOf="@+id/red_2" />

    <Button
        android:id="@+id/blue_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:background="@android:color/holo_blue_dark"
        android:text="青４番"
        app:layout_constraintStart_toEndOf="@+id/red_1"
